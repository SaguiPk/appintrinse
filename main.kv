#:import utils kivy.utils

MDFloatLayout:
    canvas:
        Color:
            rgba: utils.get_color_from_hex('#8214EB')
        RoundedRectangle:
            pos: self.pos
            size: self.size
            radius: [0, 0, 0, 0]


    Image:
        pos_hint: {"right": 0.99, "top": 0.2}
        size_hint: 0.15, 0.15
        source: "icones/intrinse.png"

    Label:
        text: f"{' '*25}[size=20][b]INTRINSE[/b][/size]\n[size=12]Seu sentimento Ã© nossa prioridade[/size]"
        pos_hint: {"right":0.78 ,"top":0.17}
        size_hint: 0.1, 0.1
        font_name: 'Gotham-Rounded-Medium'
        markup: True


    ScreenManager
        id: screen_manager
        pos_hint: {"right": 1, "top": 1}
        size_hint: 1, 1

        HomePage:
            name: "homepage"
            id: homepage

        Psicos:
            name: "psicos"
            id: psicos

        NomeCliente:
            name: "nomecliente"
            id: nomecliente

        EnvioMsg:
            name: "enviomsg"
            id: enviomsg



